<% @owners.each do |owner| %>

<p>Name: <%= owner.owner_name %></p>
<p>Adopted: <ul>
   <% owner.animals.each do |animal| %>
   <li><%= animal.animal_name %></li>
   <% end %></ul></p>
<form method="post" action="/rabbit-rescue/<%= owner.id %>/delete">
  <input type="submit" value="Delete Entry"/>
</form>

<% end %>
