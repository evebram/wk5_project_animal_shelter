<form method="post" action="/rabbit-rescue">
  <div class="form-wrap">

<div class="form-item">
<label for="name-field">Name:</label><br>
<input type="text"
 name="animal_name"
 id="name-field"/>
 </div>

 <div class="form-item">
 <label for="breed-field">Select breed:</label><br>
   <select name="breed" id="breed-field">
     <option value="Angora">Angora</option>
     <option value="Belgian Hare">Belgian Hare</option>
     <option value="Californian">Californian</option>
     <option value="Continental Giant">Continental Giant</option>
     <option value="Dutch">Dutch</option>
     <option value="English Lop">English Lop</option>
     <option value="Flemish Giant">Flemish Giant</option>
     <option value="French Lop">French Lop</option>
     <option value="Harlequin">Harlequin</option>
     <option value="Lionhead">Lionhead</option>
     <option value="Mini-lop">Mini-lop</option>
     <option value="Netherlands Dwarf">Netherlands Dwarf</option>
     <option value="Rex">Rex</option>
   </select>
   </div>

  <div class="form-item">
    <label for="age-field">Age:</label><br>
    <input
    type="text"
    name="age"
    id="age-field"/>
  </div>

 <div class="form-item">
   <label for="admission-date-field">Admission date:</label><br>
   <input
   type="text"
   name="admission_date"
   id="admission-date-field"/>
 </div>

 <div class="form-item">
 <label for="adoption-status">Ready for adoption?</label><br>
 <select name="status" id="adoption-status">
     <option value="Yes">Yes</option>
     <option value="No" selected>No</option>
 </select>
 </div>

 <div class="form-item">
 <label for="assign-owner">Owner:</label><br>
  <select name="owner_id" id="assign-owner">
    <option value="" >None</option>
    </select>
    </div>

    <div class="form-item">
    <label for="image-field">Image:</label><br>
    <input
    type="text"
    name="image"
    id="image-field"
    value=""
    />
    </div>

    <div class="form-item">
 <input type="submit" value="Register Bunny">
 </div>

 </div>

</form>
